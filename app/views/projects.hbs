<h2>Проекты</h2>

<table style="width:100%">
	{{#each projects}}
		<tr>
	    	<td><a class="project-link" href="#">{{this.name}}</a></td>
    		<td><span class="project-base-url">эндпоинтов {{this.endpoints.length}} | {{this.baseUrl}}</span></td>
    		<td><a class="edit-project" href="#">Изменить</a></td>
    		<td><a class="delete-project" href="#">Удалить</a></td>
	    </tr>
	{{/each}}
</table>

<br />

<a class="add-project modern-button" href="#">Добавить</a>
